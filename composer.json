{
    "name":         "chebur/symfony-skeleton",
    "license":      "proprietary",
    "type":         "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.1.3",

        "ext-redis": "^3.1",

        "symfony/symfony":                      "3.4.*",
        "doctrine/doctrine-bundle":             "^1.6",
        "doctrine/orm":                         "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle":           "^5.0.19",
        "sensio/framework-extra-bundle":        "^5.0.0",
        "symfony/monolog-bundle":               "^3.1.0",
        "symfony/polyfill-apcu":                "^1.0",
        "symfony/swiftmailer-bundle":           "^3.1",
        "twig/twig":                            "^2.0",

        "alexpts/symfony-asset-strategy-last-modify": "^2.0",
        "beberlei/DoctrineExtensions":                "^1.0",
        "browscap/browscap-php":                      "^4.0",
        "chebur/login-form-bundle":                   "^0.0.12",
        "chebur/search-bundle":                       "^0.0.1",
        "chebur/sphinx-bundle":                       "^0.0.5",
        "chebur/twig-php-functions-bundle":           "^0.0.2",
        "craue/formflow-bundle":                      "^3.0",
        "doctrine/doctrine-fixtures-bundle":          "^3.0",
        "doctrine/doctrine-migrations-bundle":        "^1.2",
        "egeloen/ckeditor-bundle":                    "^6.0",
        "emanueleminotto/twig-cache-bundle":          "^1.0",
        "excelwebzone/recaptcha-bundle":              "^1.4",
        "friendsofsymfony/jsrouting-bundle":          "^2.0",
        "friendsofsymfony/user-bundle":               "^2.0",
        "gedmo/doctrine-extensions":                  "^2.4",
        "gregwar/captcha-bundle":                     "^2.0",
        "hwi/oauth-bundle":                           "^0.6.1",
        "jms/serializer-bundle":                      "^2.0",
        "knplabs/knp-menu-bundle":                    "^2.1",
        "knplabs/knp-paginator-bundle":               "^2.6",
        "knplabs/knp-snappy-bundle":                  "^1.4",
        "knplabs/knp-time-bundle":                    "^1.7",
        "liip/imagine-bundle":                        "^1.8",
        "martinlindhe/php-mb-helpers":                "^0.1.6",
        "matthiasnoback/symfony-console-form":        "^2.3",
        "mobiledetect/mobiledetectlib":               "^2.8",
        "php-amqplib/rabbitmq-bundle":                "^1.13",
        "php-http/httplug-bundle":                    "^1.8",
        "php-http/curl-client":                       "^1.7",
        "php-http/guzzle6-adapter":                   "^1.1",
        "php-http/socket-client":                     "^1.4",
        "smart-core/accelerator-cache-bundle":        "^1.3",
        "snc/redis-bundle":                           "^2.0",
        "syslogic/doctrine-json-functions":           "^2.0",
        "twig/extensions":                            "^1.5",

        "npm-asset/bootstrap":     "^4.1",
        "bower-asset/fontawesome": "^4.7",
        "bower-asset/jquery":      "^3.2",
        "bower-asset/jquery-form": "^4.2",

        "hirak/prestissimo":         "^0.3.7",
        "fxp/composer-asset-plugin": "^1.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge":  "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "vendor-dir":    "vendor",
        "component-dir": "vendor/components",
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library":   "public/vendor",
                "bower-asset-library": "public/vendor"
            }
        }
    },
    "extra": {
        "symfony-app-dir":        "app",
        "symfony-bin-dir":        "bin",
        "symfony-var-dir":        "var",
        "symfony-web-dir":        "public",
        "symfony-tests-dir":      "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/parameters.yml"
        },
        "branch-alias":           null
    }
}
