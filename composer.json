{
    "name":         "chebur/symfony-skeleton",
    "license":      "proprietary",
    "type":         "project",
    "autoload": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php",
            "app/AppCache.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php"
        ]
    },
    "require": {
        "php": "^7.1",
        "symfony/symfony":                      "3.3.*",
        "doctrine/doctrine-bundle":             "^1.6",
        "doctrine/orm":                         "^2.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "sensio/distribution-bundle":           "^5.0.19",
        "sensio/framework-extra-bundle":        "^3.0.2",
        "symfony/monolog-bundle":               "^3.1.0",
        "symfony/polyfill-apcu":                "^1.0",
        "symfony/swiftmailer-bundle":           "^2.3.10",
        "twig/twig":                            "^1.0||^2.0",

        "beberlei/DoctrineExtensions":         "^1.0",
        "browscap/browscap-php":               "^3.1",
        "chebur/login-form-bundle":            "^0.0.10",
        "chebur/sphinx-bundle":                "^0.0.5",
        "chebur/twig-php-functions-bundle":    "^0.0.2",
        "craue/formflow-bundle":               "~3.0@dev",
        "doctrine/doctrine-fixtures-bundle":   "^2.3",
        "doctrine/doctrine-migrations-bundle": "^1.2",
        "egeloen/ckeditor-bundle":             "^6.0",
        "excelwebzone/recaptcha-bundle":       "^1.4",
        "friendsofsymfony/jsrouting-bundle":   "^1.6",
        "friendsofsymfony/user-bundle":        "^2.0",
        "gedmo/doctrine-extensions":           "^2.4",
        "gregwar/captcha-bundle":              "^2.0",
        "hwi/oauth-bundle":                    "^0.5.3",
        "jms/serializer-bundle":               "^2.0",
        "knplabs/knp-menu-bundle":             "^2.1",
        "knplabs/knp-paginator-bundle":        "^2.6",
        "knplabs/knp-snappy-bundle":           "^1.4",
        "knplabs/knp-time-bundle":             "^1.7",
        "liip/imagine-bundle":                 "^1.8",
        "mobiledetect/mobiledetectlib":        "^2.8",
        "php-amqplib/rabbitmq-bundle":         "^1.13",
        "predis/predis":                       "^1.1",
        "snc/redis-bundle":                    "^2.0",
        "twig/extensions":                     "^1.5",

        "bower-asset/bootstrap":             "v4.0.0-beta1",
        "bower-asset/fontawesome":           "^4.7",
        "bower-asset/foundation-icon-fonts": "~3.0@dev",
        "bower-asset/glyphicons":            "^0.0.2",
        "bower-asset/icomoon":               "^0.1.2",
        "bower-asset/ionicons":              "^2.0",
        "bower-asset/material-design-icons": "^3.0",
        "bower-asset/octicons":              "^6.0",
        "bower-asset/open-iconic":           "^1.1",

        "hirak/prestissimo":         "^0.3.7",
        "fxp/composer-asset-plugin": "^1.3"
    },
    "require-dev": {
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge":  "^3.0"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "vendor-dir":    "vendor",
        "component-dir": "vendor/components",
        "fxp-asset": {
            "installer-paths": {
                "npm-asset-library":   "web/vendor",
                "bower-asset-library": "web/vendor"
            }
        }
    },
    "extra": {
        "symfony-app-dir":        "app",
        "symfony-bin-dir":        "bin",
        "symfony-var-dir":        "var",
        "symfony-web-dir":        "web",
        "symfony-tests-dir":      "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias":           null
    }
}
